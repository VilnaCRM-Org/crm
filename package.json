{
  "name": "crm",
  "version": "0.2.0",
  "packageManager": "pnpm@9.15.3",
  "private": true,
  "scripts": {
    "dev": "craco start",
    "build": "craco build",
    "git-hooks-install": "husky install",
    "prepare": "husky install",
    "storybook-start": "storybook dev -p 6006",
    "storybook-build": "storybook build",
    "generate-ts-doc": "api-extractor run --local --verbose",
    "format": "prettier --write .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit --newLine LF",
    "lint:md": "markdownlint -i CHANGELOG.md **/*.md",
    "test:e2e": "cypress run --headless",
    "test:e2e-local": "cypress open",
    "test:unit": "jest --verbose",
    "test:memory-leak": "memlab run -v --scenario ./memlab/memoryLeakTest.js",
    "lighthouse:desktop": "lhci autorun --config=lighthouserc.desktop.js",
    "lighthouse:mobile": "lhci autorun --config=lighthouserc.mobile.js",
    "check-node-version": "node checkNodeVersion.js",
    "preinstall": "node checkNodeVersion.js"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@craco/craco": "^7.1.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "@mui/system": "^7.3.1",
    "@reduxjs/toolkit": "^2.0.1",
    "@sentry/react": "^7.120.3",
    "dotenv": "^16.5.0",
    "graphql": "^16.11.0",
    "i18next": "^23.7.16",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.49.3",
    "react-i18next": "^14.0.0",
    "react-redux": "^9.1.0",
    "react-router-dom": "^6.21.2",
    "react-scripts": "^5.0.1",
    "reflect-metadata": "^0.2.1",
    "require-from-string": "^2.0.2",
    "tsyringe": "^4.10.0",
    "wait-on": "^8.0.3",
    "web-vitals": "^3.5.2",
    "winston": "^3.17.0",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@commitlint/cli": "^17.8.1",
    "@commitlint/config-conventional": "^17.8.1",
    "@lhci/cli": "^0.10.0",
    "@microsoft/api-extractor": "^7.52.8",
    "@storybook/addon-actions": "^7.6.20",
    "@storybook/addon-essentials": "^7.6.20",
    "@storybook/addon-interactions": "^7.6.20",
    "@storybook/addon-links": "^7.6.20",
    "@storybook/react-webpack5": "^7.6.20",
    "@storybook/testing-library": "^0.2.2",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "20.19.11",
    "@types/react": "18.3.24",
    "@types/react-dom": "^18.3.7",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "babel-loader": "^9.2.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "css-loader": "^7.1.2",
    "cypress": "^13.6.2",
    "eslint": "8.56.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-cypress": "^2.15.2",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.4",
    "markdownlint-cli": "^0.38.0",
    "memlab": "^1.1.58",
    "prettier": "^3.2.0",
    "sass": "^1.89.2",
    "sass-loader": "^13.3.3",
    "storybook": "^7.6.20",
    "style-loader": "^3.3.4",
    "ts-jest": "^29.3.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9"
  },
  "engineStrict": true,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
